@model IEnumerable<BAU.Models.Employee>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>Employee Web API List</h4>

<p class="label-info">
    This comes from the API WEB.. Which can also use to insert and delete and run many more ways to access it.
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Employee_ID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Total_No_Shift)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Last_Shift)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Lock)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Employee_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Total_No_Shift)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Last_Shift)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lock)
            </td>

        </tr>
    }

</table>
